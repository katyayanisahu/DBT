CREATE OR REPLACE TABLE &quot;TRANSFORMATION&quot;.&quot;PUBLIC&quot;.&quot;Transform_join_REVIEWS&quot;
AS
SELECT
A.CUSTOMER_ID,A.CUSTOMER_NAME,A.STATUS,A.RATING,A.REMARKS_BY_UNHAPPY_CUSTO
MERS AS REMARKS FROM
&quot;VWE_S3quot;.&quot;X_POSTGRES_RDS_RAJSHRI&quot;.&quot;REVIEW_NEW&quot; as A
INNER JOIN &quot;VWE_DATABASE&quot;.&quot;X_POSTGRES_RDS_RAJSHRI&quot;.&quot;REVIEWS&quot; as B ON
A.CUSTOMER_ID = B.CUSTOMER_ID;